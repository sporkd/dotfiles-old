[user]
  name = <%= print("Your Name: "); STDOUT.flush; STDIN.gets.chomp %>
  email = <%= print("Your Email: "); STDOUT.flush; STDIN.gets.chomp %>
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  autocrlf = input
  excludesfile = ~/.gitignore
[apply]
  whitespace = nowarn
[format]
  pretty = %Cblue%h%Creset %Cgreen[%cr]%Creset (%an) %s
[alias]
  dic = diff --cached
  plre = pull --rebase
  out = log origin..HEAD
  qpl = log --pretty=\"%Cblue%h%Creset %Cgreen[%cr]%Creset %s\" origin/production..origin/master
  graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  uncommit = reset --soft HEAD^
  unstage = reset HEAD
  unmerge = reset --hard
  top = rev-parse --show-cdup
